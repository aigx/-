package com.agx.maze;

public class UserMaze {

	public static void main(String[] args) {
		Maze maze = new Maze();
		System.out.println(maze);
		boolean b = maze.trygo(0, 0);
		if(b == true){
			System.out.println("成功通过迷宫");
		}else if(b ==false){
			System.out.println("迷宫通过失败");
		}
		System.out.println(maze);
		
	}
}
